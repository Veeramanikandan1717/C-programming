// Factorial  of  a number 

#include<stdio.h>


int factorial(num){
    if (num<=1)
        return 1;
    return num*factorial(num-1);
}

int factorial(int a );

int main(void) {
    int num;
    
    scanf("%d",&num);
    int result;
    result=factorial(num);
    printf("Factorial of a number is    :   %d",result);
    
    return 0;
}
